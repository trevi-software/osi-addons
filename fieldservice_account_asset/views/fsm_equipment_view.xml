<odoo>

    <record id="fsm_equipment_form_view_inherit" model="ir.ui.view">
        <field name="model">fsm.equipment</field>
        <field name="inherit_id"
               ref="fieldservice.fsm_equipment_form_view"/>
        <field name="arch" type="xml">
            <notebook position="inside">
                <page string="Accounting">
                    <group col="2">
                        <group>
                            <field name="asset_id" readonly="1"/>
                            <field name="asset_location_id" readonly="1"/>
                        </group>
                    </group>
                </page>
            </notebook>
        </field>
    </record>

</odoo>
